{
  "name": "webpack-config",
  "version": "1.0.0",
  "description": "Кастомный конфиг Webpack",
  "author": "Askar Ialiaev <askarrussia@gmail.com>",
  "license": "MIT",
  "private": true,
  "browserslist": "> 0.25%, not dead",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --mode development --open",
    "dev-build": "cross-env NODE_ENV=development webpack --mode development",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "deploy": "yarn build && gh-pages -d build",
    "precommit": "yarn eslint ./src"
  },
  "devDependencies": {
